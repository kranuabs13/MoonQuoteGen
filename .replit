packages = ["chromium", "glib", "nspr", "nss", "dbus", "atk", "gtk3", "pango", "cairo", "gdk-pixbuf", "mesa", "cups"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
